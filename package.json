{
  "author": "Anton Trofymenko",
  "bugs": {
    "url": "https://github.com/r37r0m0d3l/punycoder/issues"
  },
  "contributors": [
    {
      "email": "r37r0m0d3l@protonmail.com",
      "name": "Anton Trofymenko"
    }
  ],
  "description": "Punycode converter.",
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": ">=8.12.0"
  },
  "engineStrict": true,
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/index.js"
      },
      "import": "./dist/index.js",
      "node": {
        "import": "./dist/index.js"
      },
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map"
  ],
  "homepage": "https://github.com/r37r0m0d3l/punycoder",
  "keywords": [
    "ASCII",
    "dns",
    "domain",
    "encoding",
    "idn",
    "idna",
    "punycode",
    "punycode",
    "transcode",
    "unicode",
    "unicode",
    "url"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "name": "puny-coder",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r37r0m0d3l/punycoder.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --sourcemap --clean --minify --treeshake",
    "dev": "tsup src/index.ts --format esm --watch --dts",
    "package:upgrade": "npx npm-check --update",
    "prepublishOnly": "npm run build",
    "test": "node --test"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "2.0.0"
}
